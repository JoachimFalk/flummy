<?xml version="1.0"?>
<!DOCTYPE vpcconfiguration SYSTEM "cmx.dtd">
<vpcconfiguration>
 <resultfile name=""/>
 <measurefile name=""/>

 <resources>
  <component name="Component1" type="threaded" scheduler="RoundRobin">
   <attribute type="timeslice" value="20ns"/>
  </component>

  <component name="ZeroCom" type="threaded" scheduler="FCFS">
   <attribute type="transaction_delay" value="0 ns" />
  </component>
 </resources>

 <mappings>
  <mapping source="sqrroot.a1" target="Component1">
    <timing delay="100"/>
    <timing fname="Src::src" delay="100"/>
  </mapping>

  <mapping source="sqrroot.a2" target="Component1">
    <timing delay="100"/>
    <timing fname="SqrLoop::copyStore"  delay="100"/>
    <timing fname="SqrLoop::copyInput"  delay="100"/>
    <timing fname="SqrLoop::copyApprox" delay="100"/>
  </mapping>

  <mapping source="sqrroot.a3" target="Component1">
    <timing delay="100"/>
    <timing fname="Approx::approx" delay="100"/>
  </mapping>

  <mapping source="sqrroot.a4" target="Component1">
    <timing delay="100"/>
    <timing fname="Dup::dup" delay="100"/>
  </mapping>

  <mapping source="sqrroot.a5" target="Component1">
    <timing delay="100"/>
    <timing fname="Sink::sink" delay="100"/>
  </mapping>

  <mapping source="cf_sqrroot.a1_sqrroot.a2_1" target="ZeroCom">
    <timing delay="0"/>
  </mapping>

  <mapping source="cf_sqrroot.a2_sqrroot.a3_1" target="ZeroCom">
    <timing delay="0"/>
  </mapping>

  <mapping source="cf_sqrroot.a3_sqrroot.a4_1" target="ZeroCom">
    <timing delay="0"/>
  </mapping>

  <mapping source="cf_sqrroot.a4_sqrroot.a3_1" target="ZeroCom">
    <timing delay="0"/>
  </mapping>

  <mapping source="cf_sqrroot.a4_sqrroot.a2_1" target="ZeroCom">
    <timing delay="0"/>
  </mapping>

  <mapping source="cf_sqrroot.a2_sqrroot.a5_1" target="ZeroCom">
    <timing delay="0"/>
  </mapping>

 </mappings>
 <topology>
  <route source="sqrroot.a1" destination="cf_sqrroot.a1_sqrroot.a2_1">
    <hop name="ZeroCom" />
  </route>
  <route source="cf_sqrroot.a1_sqrroot.a2_1" destination="sqrroot.a2">
    <hop name="ZeroCom" />
  </route>
  <route source="sqrroot.a2" destination="cf_sqrroot.a2_sqrroot.a3_1">
    <hop name="ZeroCom" />
  </route>
  <route source="cf_sqrroot.a2_sqrroot.a3_1" destination="sqrroot.a3">
    <hop name="ZeroCom" />
  </route>
  <route source="cf_sqrroot.a4_sqrroot.a3_1" destination="sqrroot.a3">
    <hop name="ZeroCom" />
  </route>
  <route source="sqrroot.a3" destination="cf_sqrroot.a3_sqrroot.a4_1">
    <hop name="ZeroCom" />
  </route>
  <route source="cf_sqrroot.a3_sqrroot.a4_1" destination="sqrroot.a4">
    <hop name="ZeroCom" />
  </route>
  <route source="sqrroot.a4" destination="cf_sqrroot.a4_sqrroot.a3_1">
    <hop name="ZeroCom" />
  </route>
  <route source="sqrroot.a4" destination="cf_sqrroot.a4_sqrroot.a2_1">
    <hop name="ZeroCom" />
  </route>
  <route source="cf_sqrroot.a4_sqrroot.a2_1" destination="sqrroot.a2">
    <hop name="ZeroCom" />
  </route>
  <route source="sqrroot.a2" destination="cf_sqrroot.a2_sqrroot.a5_1">
    <hop name="ZeroCom" />
  </route>
  <route source="cf_sqrroot.a2_sqrroot.a5_1" destination="sqrroot.a5">
    <hop name="ZeroCom" />
  </route>
 </topology>
</vpcconfiguration>
