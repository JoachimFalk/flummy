<?xml version="1.0"?>
<!DOCTYPE vpcconfiguration SYSTEM "cmx.dtd">
<vpcconfiguration>

 <resources>
  <component name="CPU" scheduler="PS">
   <attribute type="powermode">
     <attribute type="FAST">
	   <parameter type="IDLE"     value="0.05 mW" />
	   <parameter type="STALLED"  value="0.50 mW" />
	   <parameter type="RUNNING"  value="1.00 mW" />
	   <parameter type="transaction_delay" value="1 ns"/>
	 </attribute>
	 <attribute type="SLOW">
	   <parameter type="IDLE"     value="0.01 mW" />
	   <parameter type="STALLED"  value="0.11 mW" />
	   <parameter type="RUNNING"  value="0.21 mW" />
	   <parameter type="transaction_delay" value="3 ns"/>
	 </attribute>
   </attribute>
  </component>

  <component name="Bus" scheduler="PS">
   <attribute type="powermode">
     <attribute type="FAST">
	   <parameter type="IDLE"     value="0.06 mW" />
	   <parameter type="STALLED"  value="0.60 mW" />
	   <parameter type="RUNNING"  value="1.00 mW" />
	   <parameter type="transaction_delay" value="1 ns"/>
	 </attribute>
	 <attribute type="SLOW">
	   <parameter type="IDLE"     value="0.02 mW" />
	   <parameter type="STALLED"  value="0.12 mW" />
	   <parameter type="RUNNING"  value="0.22 mW" />
	   <parameter type="transaction_delay" value="3 ns"/>
	 </attribute>
   </attribute>
  </component>

  <component name="Mem" scheduler="PS">
    <attribute type="powermode">
     <attribute type="FAST">
	   <parameter type="IDLE"     value="0.07 mW" />
	   <parameter type="STALLED"  value="0.70 mW" />
	   <parameter type="RUNNING"  value="1.00 mW" />
	   <parameter type="transaction_delay" value="1 ns"/>
	 </attribute>
	 <attribute type="SLOW">
	   <parameter type="IDLE"     value="0.03 mW" />
	   <parameter type="STALLED"  value="0.13 mW" />
	   <parameter type="RUNNING"  value="0.23 mW" />
	   <parameter type="transaction_delay" value="3 ns"/>
	 </attribute>
   </attribute>
  </component>
 </resources>


 <mappings>
  <mapping source="sqrroot.a1" target="CPU">
    <timing delay="1 ns"  powermode="FAST"/>
    <timing delay="10 ns" powermode="SLOW"/>
    <timing fname="Src::src" delay="1 ns"   powermode="FAST"/>
    <timing fname="Src::src" delay="10 ns"  powermode="SLOW"/>
  </mapping>

  <mapping source="sqrroot.a2" target="CPU">
	<timing delay="1 ns"  powermode="FAST"/>
    <timing delay="10 ns" powermode="SLOW"/>
    <timing fname="Sink::sink" delay="23 ns"  powermode="FAST"/>
    <timing fname="Sink::sink" delay="27 ns"  powermode="SLOW"/> 
  </mapping>

  <mapping source="sqrroot.a3" target="CPU">
    <timing delay="1 ns"  powermode="FAST"/>
    <timing delay="10 ns" powermode="SLOW"/>
  </mapping>
 </mappings>
 
 
 <topology tracing="true" default="ignore">
  <route source="sqrroot.a1" destination="cf_sqrroot.a1_sqrroot.a2_1" tracing="false">
    <hop name="CPU" />
    <hop name="Bus" />
    <hop name="Mem" />
  </route>
  <route source="cf_sqrroot.a1_sqrroot.a2_1" destination="sqrroot.a2">
    <hop name="Mem" />
    <hop name="Bus" />
    <hop name="CPU" />
  </route>
 </topology>
 
</vpcconfiguration>
