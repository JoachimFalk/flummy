<?xml version="1.0"?>
<!DOCTYPE vpcconfiguration SYSTEM "cmx.dtd">
<vpcconfiguration>
 <resultfile name=""/>
 <measurefile name=""/>

 <resources>
  <component name="SG1" type="threaded" scheduler="PS">
    <attribute type="transaction_delay" value="50us" />
  </component>

  <component name="SG2" type="threaded" scheduler="PS">
    <attribute type="transaction_delay" value="8us" />
  </component>

  <component name="SG3" type="threaded" scheduler="PS">
    <attribute type="transaction_delay" value="1us" />
  </component>
  
  
  <component name="Switch1" type="threaded" scheduler="PS">
  	<attribute type="transaction">
  		<parameter type="delay" value="0.083333336 us" />
    	<parameter type="size" value="1" />
  	</attribute>
  </component>
  <component name="Switch2" type="threaded" scheduler="FCFS">
    <attribute type="transaction">
        <parameter type="delay" value="0.083333336 us" />
        <parameter type="size" value="1" />
    </attribute>
  </component>
  
  <component name="sg1_eth1" scheduler="AVB">
    <attribute type="1" value="0.5"></attribute>
    <attribute type="2" value="0.2"></attribute>
    <attribute type="transaction">
        <parameter type="delay" value="2 us" />
        <parameter type="size" value="1" />
    </attribute>
  </component>
  <component name="eth1_eth2" scheduler="AVB">
    <attribute type="1" value="0.2"></attribute>
    <attribute type="2" value="0.5"></attribute>
    <attribute type="transaction">
        <parameter type="delay" value="2 us" />
        <parameter type="size" value="1" />
    </attribute>
  </component>
  <component name="eth2_sg2" scheduler="AVB">
    <attribute type="1" value="0.5"></attribute>
    <attribute type="2" value="0.2"></attribute>
    <attribute type="transaction">
        <parameter type="delay" value="2 us" />
        <parameter type="size" value="1" />
    </attribute>
  </component>
  
  <component name="ZeroCom" scheduler="PS">
    <attribute type="transaction_delay" value="0us" />
  </component>

  <component name="ZeroCom2" scheduler="PS">
    <attribute type="transaction_delay" value="0us" />
  </component>

 </resources>

 <mappings>
  <mapping source="TTGraph.Producer_44,1kHz" target="SG1">
    <timing delay="1us"/>
  </mapping>

  <mapping source="TTGraph.Player_44,1kHz" target="SG2">
    <timing delay="5us"/>
  </mapping>

  <mapping source="TTGraph.Send_1722" target="SG1">
    <timing delay="5us"/>
  </mapping>
  
  <mapping source="TTGraph.Receive_1722" target="SG2">      
    <timing delay="5us"/>
  </mapping>  
  <mapping source="TTGraph.Traffic1" target="eth1_eth2">
    <timing delay="114.4us"/>
  </mapping>
    <mapping source="TTGraph.Traffic2" target="eth1_eth2">
    <timing delay="114.4us"/>
  </mapping>
    <mapping source="TTGraph.Traffic3_consume" target="ZeroCom">
    <timing delay="4us"/>
  </mapping>  
  <mapping source="TTGraph.Traffic3_send" target="ZeroCom">
    <timing delay="1us"/>
  </mapping>
 </mappings>
 <topology tracing="true">
 
   <route  source="TTGraph.Producer_44,1kHz" destination="channel1" tracing="false">
   </route>   
   <route  source="channel1" destination="TTGraph.Send_1722">
   </route>

   <route  source="TTGraph.Send_1722" destination="channel2_2">
	 <hop name="sg1_eth1">  <timing dii="8.39us" latency="8.39us" />  <attribute type="priority" value="2" />   
     </hop>
	 <hop name="Switch1">  <timing dii="17.49ns" latency="17.49ns" /> </hop>
	 <hop name="eth1_eth2">  <timing dii="8.39us" latency="8.39us" /> <attribute type="priority" value="2" /> </hop>
   	 <hop name="Switch2">  <timing dii="17.49ns" latency="17.49ns" /> </hop>
   	 <hop name="eth2_sg2">  <timing dii="8.39us" latency="8.39us" /> <attribute type="priority" value="2" /> </hop>
   </route>
   <route  source="channel2_2" destination="TTGraph.Receive_1722">
   </route>

   <route  source="TTGraph.Send_1722" destination="channel2_1">
	 <hop name="sg1_eth1">  <timing dii="7.78us" latency="7.78us" />  <attribute type="priority" value="2" />   
     </hop>
	 <hop name="Switch1">  <timing dii="16.21ns" latency="16.21ns" /> </hop>
	 <hop name="eth1_eth2">  <timing dii="7.78us" latency="7.78us" /> <attribute type="priority" value="2" /> </hop>
   	 <hop name="Switch2">  <timing dii="16.21ns" latency="16.21ns" /> </hop>
   	 <hop name="eth2_sg2">  <timing dii="7.78us" latency="7.78us" /> <attribute type="priority" value="2" /> </hop>
   </route>
   <route  source="channel2_1" destination="TTGraph.Receive_1722">
   </route>

   
   <route  source="TTGraph.Receive_1722" destination="channel3">
   </route>
   <route  source="channel3" destination="TTGraph.Player_44,1kHz">
   </route>
   
   <route  source="TTGraph.Traffic3_send" destination="channelTraffic3" tracing="true">
	 <hop name="Switch1">  <timing dii="8ns" latency="8ns" /> </hop>
	 <hop name="eth1_eth2">  <timing dii="4us" latency="4us" /> <attribute type="priority" value="2" /> </hop>
   	 <hop name="Switch2">  <timing dii="8ns" latency="8ns" /> </hop>
	 <hop name="ZeroCom2">  <timing dii="8ns" latency="8ns" /> </hop>
   </route>
   <route  source="channelTraffic3" destination="TTGraph.Traffic3_consume">
   </route>
   
  </topology>
</vpcconfiguration>
